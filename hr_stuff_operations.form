<?xml version="1.0"  encoding="utf-8"  standalone="yes"?><!DOCTYPE xml>
<Hlayout spacing="0" class="valignMiddle">
	<Image width="35px" height="35px" visible="boolean:P{refresh|true}" src="P{TEGSOFT_THEME}/tegsoft-refresh.png" dataSourceName="TBLHRSTUFF" dataSourceFunction="refresh" tooltiptext="${dataSourceButton.refresh.caption}" style="String:cursor:pointer" />
	<Image width="35px" height="35px" visible="boolean:P{undo|true}" src="P{TEGSOFT_THEME}/tegsoft-undo.png" dataSourceName="TBLHRSTUFF" dataSourceFunction="undo" tooltiptext="${dataSourceButton.undo.caption}" style="String:cursor:pointer" />
	<Image width="35px" height="35px" visible="boolean:P{insert|true}" src="P{TEGSOFT_THEME}/tegsoft-add.png" dataSourceName="TBLHRSTUFF" dataSourceFunction="insert" tooltiptext="${dataSourceButton.insert.caption}" style="String:cursor:pointer" />
